tail -f /etc/passwd & | awk -F':' '{print $1}' awk -F':' '{print $3}' | grep ':/' | awk -F':' '{print $6}'


cat /etc/passwd | awk -F':' '{print $1, $3, $7}'

cat /etc/passwd | awk -F: '{print $1 "\t\t" $3 "\t" $6}'







# Write a script which displays only the login, UID and Path of each entry of the
# /etc/passwd file.

